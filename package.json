{
  "name": "sliiides",
  "version": "0.2.0",
  "type": "module",
  "homepage": "https://slides.oddbird.net",
  "title": "Sliiides",
  "repository": {
    "type": "git",
    "url": "https://github.com/oddbird/sliiides.git"
  },
  "private": true,
  "scripts": {
    "watch:sass": "sass --pkg-importer=node --watch _src/sass:_site/assets/css",
    "build:sass": "sass --pkg-importer=node src/scss:_site/assets/css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "serve": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^5.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "@terriblemia/ground-control": "^0.3.0",
    "accoutrement": "^4.0.5",
    "cascading-color-systems": "^0.1.0-beta.17",
    "cssremedy": "^0.1.0-beta.2",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "js-yaml": "^4.1.0",
    "markdown-it-mark": "^4.0.0",
    "netlify-plugin-cache": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prismjs": "^1.29.0",
    "sass": "^1.86.1"
  },
  "dependencies": {
    "@oddbird/browser-support": "^0.1.0",
    "@oddbird/slide-deck": "^0.2.0-rc.1",
    "lite-youtube-embed": "^0.3.0",
    "lodash-es": "^4.17.21"
  }
}
